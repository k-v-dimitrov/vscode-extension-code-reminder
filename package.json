{
  "name": "code-remind",
  "displayName": "Code Remind",
  "description": "A simple extensions that let's you set in-code reminders.",
  "version": "0.0.2",
  "publisher": "flamingsoul2",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-remind.createReminder",
        "title": "Reminder: Put reminder on current line"
      },
      {
        "command": "code-remind.getReminders",
        "title": "Reminder: Get current reminders"
      },
      {
        "command": "code-remind.refreshRemindersTreeView",
        "title": "Reminder: Refresh tree reminders view"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "code-remind.createReminder",
          "group": "reminders"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "reminders",
          "name": "Reminders",
          "icon": "/src/assets/pin.png"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "clean": "rimraf dist && rimraf node_modules"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/node-cron": "^3.0.1",
    "@types/vscode": "^1.64.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vscode/test-electron": "^2.1.2",
    "copy-webpack-plugin": "^10.2.4",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@types/uuid": "^8.3.4",
    "events": "^3.3.0",
    "joi": "^17.6.0",
    "node-cron": "^3.0.0",
    "uuid": "^8.3.2"
  }
}
